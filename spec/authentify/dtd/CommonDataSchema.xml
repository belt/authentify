<?xml version='1.0' encoding = "ISO-8859-1"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"		targetNamespace="http://xml.authentify.net/CommonDataSchema.xml"	xmlns:tns="http://xml.authentify.net/CommonDataSchema.xml" 	elementFormDefault="qualified"	attributeFormDefault="unqualified"><xsd:annotation>	<xsd:documentation xml:lang="en">The common input and output data schema for the Authentify xml interface.</xsd:documentation></xsd:annotation><!-- Input data ====================================================== --><!-- ================================================================= --><xsd:element name="redirectURL" type="xsd:string"/><xsd:simpleType name="confidenceType">	<xsd:restriction base="xsd:nonNegativeInteger">		<xsd:maxInclusive value='100'/>	</xsd:restriction></xsd:simpleType><xsd:complexType name="namedStringType">	<xsd:annotation>		<xsd:documentation xml:lang="en">An element of a form <!-- <element name="name"> value </element> --></xsd:documentation>	</xsd:annotation>	<xsd:simpleContent>		<xsd:extension base="xsd:string">			<xsd:attribute name="name" type="xsd:string" use="required"/>			<xsd:attribute name="confidence" type="tns:confidenceType" use="optional"/>		</xsd:extension>	</xsd:simpleContent></xsd:complexType><xsd:complexType name="namedDataType">	<xsd:annotation>		<xsd:documentation xml:lang="en">			The data to be matched against the spoken and/or dtmf input.		</xsd:documentation>	</xsd:annotation>	<xsd:sequence>		<xsd:element name="dataItem" type="tns:namedStringType" minOccurs="1" maxOccurs="unbounded"/>	</xsd:sequence></xsd:complexType><xsd:element name="namedData" type="tns:namedDataType"/><xsd:simpleType name="securityLevelType">	<xsd:annotation>		<xsd:documentation xml:lang="en">Voice biometrics verification security ("confidence") level</xsd:documentation>	</xsd:annotation>	<xsd:restriction base="xsd:integer">		<xsd:enumeration value='1'/> <!-- 'Very High' -->		<xsd:enumeration value='2'/> <!-- 'High'-->		<xsd:enumeration value='3'/> <!-- 'Medium High' -->		<xsd:enumeration value='4'/> <!-- 'Medium' -->		<xsd:enumeration value='5'/> <!-- 'Medium Low' -->	</xsd:restriction></xsd:simpleType><xsd:element name="securityLevel" type="tns:securityLevelType"/><xsd:element name="language" type="xsd:language"/><xsd:simpleType name="inServiceIndicatorType">	<xsd:annotation>		<xsd:documentation xml:lang="en">InService query key to determine if the number in question has been in service for the specified period of time</xsd:documentation>	</xsd:annotation>	<xsd:restriction base="xsd:integer">		<xsd:enumeration value='1'/> <!-- 'In Service 1 Day And Over' -->		<xsd:enumeration value='2'/> <!-- 'In Service 2 Months And Over'-->		<xsd:enumeration value='3'/> <!-- 'In Service 6 Months And Over' -->		<xsd:enumeration value='4'/> <!-- 'In Service 12 Months And Over' -->		<xsd:enumeration value='5'/> <!-- 'In Service 24 Months And Over' -->	</xsd:restriction></xsd:simpleType><xsd:element name="inServiceIndicator" type="tns:inServiceIndicatorType"/><xsd:element name="speakerID" type="xsd:string"/><!-- Input/Output data =============================================== --><!-- ================================================================= --><xsd:element name="phoneNumber" type="xsd:string"/><xsd:element name="confirmNumber" type="xsd:string"/><xsd:element name="asip" type="xsd:string"/><xsd:element name="VID" type="xsd:string"/><xsd:element name="deviceContact" type="tns:deviceContactType"/><xsd:element name="relationshipId" type="tns:relationshipIdType"/><xsd:complexType name="deviceContactType">	<xsd:simpleContent>		<xsd:extension base="xsd:string">			<xsd:attribute name="registered" type="xsd:boolean" />		</xsd:extension>	</xsd:simpleContent></xsd:complexType><xsd:complexType name="relationshipIdType">	<xsd:simpleContent>		<xsd:extension base="xsd:string">           <xsd:attribute name="clientType" type="xsd:string" />           <xsd:attribute name="expires" type="xsd:string" />		</xsd:extension>	</xsd:simpleContent></xsd:complexType><xsd:simpleType name="matchType">	<xsd:annotation>		<xsd:documentation xml:lang="en">A "match" attribute description.</xsd:documentation>	</xsd:annotation>	<xsd:restriction base="xsd:integer">		<xsd:enumeration value='0'/> <!-- 'No match' -->		<xsd:enumeration value='1'/> <!-- 'Match'-->		<xsd:enumeration value='2'/> <!-- 'No data to match' -->	</xsd:restriction></xsd:simpleType><xsd:complexType name="matchStringType">	<xsd:annotation>		<xsd:documentation xml:lang="en">An element of a form <!-- <element match="match"> value </element> --></xsd:documentation>	</xsd:annotation>	<xsd:simpleContent>		<xsd:extension base="xsd:string">			<xsd:attribute name="match" type="tns:matchType" use="optional"/>		</xsd:extension>	</xsd:simpleContent></xsd:complexType><xsd:complexType name="matchCountryType">	<xsd:simpleContent>		<xsd:restriction base="tns:matchStringType">			<xsd:pattern value="[A-Z]{2}"/>		</xsd:restriction>	</xsd:simpleContent></xsd:complexType><xsd:complexType name="nameType">	<xsd:annotation>		<xsd:documentation xml:lang="en">The person's name. Either first and last or full name may be present.</xsd:documentation>	</xsd:annotation>	<xsd:choice>		<xsd:sequence>			<xsd:element name="firstName" type="tns:matchStringType" minOccurs="0"/>			<xsd:element name="lastName" type="tns:matchStringType" minOccurs="0"/>		</xsd:sequence>		<xsd:element name="fullName" type="tns:matchStringType" minOccurs="0"/>	</xsd:choice></xsd:complexType><xsd:element name="name" type="tns:nameType"/><xsd:complexType name="addressType">	<xsd:annotation>		<xsd:documentation xml:lang="en">The person's address. Subelements are optional.</xsd:documentation>	</xsd:annotation>	<xsd:sequence>		<xsd:element name="streetAddress" type="tns:matchStringType" minOccurs="0"/>		<xsd:group ref="tns:streetAddressDetailed"/>		<xsd:element name="city" type="tns:matchStringType" minOccurs="0"/>		<xsd:element name="postalCode" type="tns:matchStringType" minOccurs="0"/>		<xsd:element name="country" type="tns:matchCountryType" minOccurs="0"/>	</xsd:sequence></xsd:complexType><xsd:group name="streetAddressDetailed">	<xsd:sequence>		<xsd:element name = "streetNumber" type="tns:matchStringType" minOccurs="0"/>		<xsd:element name = "streetDirection" type="tns:matchStringType" minOccurs="0"/>		<xsd:element name = "streetName" type="tns:matchStringType" minOccurs="0"/>		<xsd:element name = "unitDesignator" type="tns:matchStringType" minOccurs="0"/>		<xsd:element name = "unitNumber" type="tns:matchStringType" minOccurs="0"/>	</xsd:sequence></xsd:group><xsd:element name="address" type="tns:addressType"/><xsd:simpleType name="XfaActionType">	<xsd:annotation>		<xsd:documentation xml:lang="en">xFA action type. Indicates what type of action this xFA session is for.</xsd:documentation> 	</xsd:annotation>	<xsd:restriction base="xsd:string">		<xsd:enumeration value="CreateRelationship" />		<xsd:enumeration value="QueryRelationship" />		<xsd:enumeration value="DeleteRelationship" />		<xsd:enumeration value="SendXfaMessage" />		<xsd:enumeration value="RequestLoginCode" />	</xsd:restriction></xsd:simpleType><xsd:simpleType name="CodeDeliveryType">	<xsd:annotation>		<xsd:documentation xml:lang="en">Describes how the one time code is delivered to end user for a CreateRelationship or RequestLogin action type.</xsd:documentation> 	</xsd:annotation>	<xsd:restriction base="xsd:string">		<xsd:enumeration value="Scan" />		<xsd:enumeration value="Phone" />		<xsd:enumeration value="Sms" />	</xsd:restriction></xsd:simpleType><xsd:simpleType name="XfaMsgType">	<xsd:restriction base="xsd:string">		<xsd:enumeration value="Inform"/>		<xsd:enumeration value="Confirm"/>		<xsd:enumeration value="Query"/>		<xsd:enumeration value="Select"/>		<xsd:enumeration value="VerifyLogin"/>	</xsd:restriction></xsd:simpleType><xsd:simpleType name="MsgTextType">	<xsd:restriction base="xsd:string">		<xsd:minLength value="1"/>		<xsd:maxLength value="500"/>	</xsd:restriction></xsd:simpleType><xsd:simpleType name="BtnTypeValue">	<xsd:restriction base="xsd:string">		<xsd:minLength value="1" />		<xsd:maxLength value="10" />	</xsd:restriction></xsd:simpleType><xsd:simpleType name="BtnTypeEnum">	<xsd:restriction base="xsd:string">		<xsd:enumeration value="Yes" />		<xsd:enumeration value="No" />	</xsd:restriction></xsd:simpleType><xsd:complexType name="BtnTextType">	<xsd:simpleContent>		<xsd:extension base="tns:BtnTypeValue">			<xsd:attribute name="type" type="tns:BtnTypeEnum"/>		</xsd:extension>	</xsd:simpleContent></xsd:complexType><xsd:simpleType name="RspInputType">	<xsd:restriction base="xsd:string">		<xsd:enumeration value="Num"/>		<xsd:enumeration value="Alpha"/>		<xsd:enumeration value="Alphanum"/>		<xsd:enumeration value="Radio"/>		<xsd:enumeration value="Dropdown"/>	</xsd:restriction></xsd:simpleType><xsd:simpleType name="XfaMsgStatusType">	<xsd:restriction base="xsd:integer">		<xsd:enumeration value="1"/> <!-- 'Acknowledged' -->		<xsd:enumeration value="2"/> <!-- 'Unexpected' -->		<xsd:enumeration value="3"/> <!-- 'Confirm' -->		<xsd:enumeration value="4"/> <!-- 'Deny' -->		<xsd:enumeration value="9999"/> <!-- 'Timeout' -->	</xsd:restriction></xsd:simpleType><xsd:simpleType name="SelEntryValue">	<xsd:restriction base="xsd:integer">		<xsd:enumeration value="0" /> 		<xsd:enumeration value="1" /> 		<xsd:enumeration value="2" /> 		<xsd:enumeration value="3" /> 		<xsd:enumeration value="4" /> 	</xsd:restriction></xsd:simpleType><xsd:complexType name="SelEntryType">	<xsd:simpleContent>		<xsd:extension base="tns:SelEntryValue">			<xsd:attribute name="selText" type="xsd:string" /> 		</xsd:extension>	</xsd:simpleContent></xsd:complexType><xsd:complexType name="SelOptionsType">	<xsd:sequence>		<xsd:element name="selItem" type="tns:SelEntryType" minOccurs="1" maxOccurs="5"/>	</xsd:sequence></xsd:complexType><xsd:complexType name="XfaMsgDataType">	<xsd:sequence>		<xsd:element name="msgText" type="tns:MsgTextType" minOccurs="1" maxOccurs="1"/>		<xsd:element name="btnText" type="tns:BtnTextType" minOccurs="0" maxOccurs="2"/>		<xsd:element name="enterpriseMsgId" type="xsd:string" minOccurs="0" maxOccurs="1"/>		<xsd:element name="maxRspChars" type="xsd:integer" minOccurs="0" maxOccurs="1"/>		<xsd:element name="rspInputType" type="tns:RspInputType" minOccurs="0" maxOccurs="1"/>		<xsd:element name="rspText" type="xsd:string" minOccurs="0" maxOccurs="1"/>		<xsd:element name="xfaMsgStatus" type="tns:XfaMsgStatusType" minOccurs="0" maxOccurs="1"/>		<xsd:element name="enterpriseHelp" type="xsd:string" minOccurs="0" maxOccurs="1"/>		<xsd:element name="selOptions" type="tns:SelOptionsType" minOccurs="0" maxOccurs="1"/>   	</xsd:sequence></xsd:complexType><xsd:complexType name="XfaDataType">	<xsd:annotation>		<xsd:documentation xml:lang="en">Sub data type encapsulating data needed for xFA sessions</xsd:documentation>	</xsd:annotation>	<xsd:sequence>		<xsd:element name="xfaAction" type="tns:XfaActionType" minOccurs="0" />		<xsd:element name="codeDeliveryType" type="tns:CodeDeliveryType" minOccurs="0" />		<xsd:element name="xfaCode" type="xsd:string" minOccurs="0" />		<xsd:element name="xfaMsgType" type="tns:XfaMsgType" minOccurs="0" maxOccurs="1"/>		<xsd:element name="xfaMsgSummary" type="xsd:string" minOccurs="0" />		<xsd:element name="actionExpiry" type="xsd:integer" minOccurs="0" />		<xsd:element name="xfaMsgData" type="tns:XfaMsgDataType" minOccurs="0" />		<xsd:element name="encryptedXfaMsgData" type="xsd:base64Binary" minOccurs="0" />		<xsd:element name="nonce" type="xsd:base64Binary" minOccurs="0" />		<xsd:element name="signedNonce" type="xsd:base64Binary" minOccurs="0" />		<xsd:element name="relationshipCert" type="xsd:string" minOccurs="0" />		<xsd:element name="relationshipExpiration" type="xsd:integer" minOccurs="0" />       <xsd:element name="appAuthExpiry" type="xsd:integer" minOccurs="0" />       <xsd:element name="appReqAuth" type="xsd:integer" minOccurs="0" />       <xsd:element name="appAuthUpdated" type="xsd:integer" minOccurs="0" />       <xsd:element name="waitForInformAck" type="xsd:boolean" minOccurs="0" />	</xsd:sequence></xsd:complexType><xsd:element name="xfaData" type="tns:XfaDataType" minOccurs="0" /><!-- Output data ===================================================== --><!-- ================================================================= --><xsd:simpleType name="hexString">   <xsd:restriction base="xsd:string">      <xsd:minLength value="1" />      <xsd:pattern value="[0-9a-fA-F]+" />   </xsd:restriction></xsd:simpleType><xsd:element name="phoneCallDuration" type="xsd:nonNegativeInteger"/><xsd:element name="phoneValidityStatus" type="tns:hexString"/><xsd:element name="phoneLookupStatus" type="tns:hexString"/><xsd:element name="zipExchangeDistance" type="xsd:decimal"/><xsd:element name="populationDensity" type="xsd:decimal"/><xsd:element name="States" type="xsd:string"/><xsd:element name="exchangeType"><xsd:simpleType>  <xsd:restriction base="xsd:string">	<xsd:enumeration value="00"/> <!-- 'Regular' -->	<xsd:enumeration value="01"/> <!-- 'Mobile (MITS)'-->	<xsd:enumeration value="02"/> <!-- 'Paging' -->	<xsd:enumeration value="03"/> <!-- 'Packet Switching' -->	<xsd:enumeration value="04"/> <!-- 'Cellular' -->	<xsd:enumeration value="05"/> <!-- 'Plant Test' -->	<xsd:enumeration value="06"/> <!-- 'Maritime' -->	<xsd:enumeration value="07"/> <!-- 'Air to Ground' -->	<xsd:enumeration value="08"/> <!-- '800 Service' -->	<xsd:enumeration value="09"/> <!-- '900 Service' -->	<xsd:enumeration value="10"/> <!-- 'Called party pays' -->	<xsd:enumeration value="11"/> <!-- 'Information provider service' -->	<xsd:enumeration value="12"/> <!-- '500 Service' -->	<xsd:enumeration value="13"/> <!-- 'Directory Assistance' -->	<xsd:enumeration value="14"/> <!-- 'Special Calling Cards' -->	<xsd:enumeration value="15"/> <!-- 'Official Exchange Carrier Service' -->	<xsd:enumeration value="16"/> <!-- 'Originating only' -->	<xsd:enumeration value="17"/> <!-- 'Billing only' -->	<xsd:enumeration value="18"/> <!-- '800 Database' -->	<xsd:enumeration value="30"/> <!-- 'Broad band' -->	<xsd:enumeration value="50"/> <!-- 'Shared amoung 3 or more services' -->	<xsd:enumeration value="51"/> <!-- 'Shared between POTS and mobile' -->	<xsd:enumeration value="52"/> <!-- 'Shared between POTS and paging' -->	<xsd:enumeration value="53"/> <!-- 'Reserved for future use' -->	<xsd:enumeration value="54"/> <!-- 'Shared between POTS and Cellular' -->	<xsd:enumeration value="55"/> <!-- 'Special billing option - Cellular' -->	<xsd:enumeration value="56"/> <!-- 'Special billing option - Paging' -->	<xsd:enumeration value="57"/> <!-- 'Special billing option - Mobile' -->	<xsd:enumeration value="58"/> <!-- 'Special billing option shared among two or more (Cellular)' -->	<xsd:enumeration value="60"/> <!-- 'Service provider request - Cellular' -->	<xsd:enumeration value="61"/> <!-- 'Service provider request - Paging' -->	<xsd:enumeration value="62"/> <!-- 'Service provider request - Mobile' -->	<xsd:enumeration value="63"/> <!-- 'Service provider request - Cellular, Paging or Mobile' -->	<xsd:enumeration value="64"/> <!-- 'Personal Communication Services' -->	<xsd:enumeration value="65"/> <!-- 'Miscellaneous service (non-500 PCS)' -->	<xsd:enumeration value="66"/> <!-- 'Shared between POTS and miscellaneous service' -->	<xsd:enumeration value="67"/> <!-- 'Special billing option - PCS / Misc Service' -->	<xsd:enumeration value="68"/> <!-- 'Service provider request - PCS / Misc' -->	<xsd:enumeration value="88"/> <!-- 'Toll station - Ring Down' -->	<xsd:enumeration value="99"/> <!-- 'No Information Available' -->  </xsd:restriction></xsd:simpleType></xsd:element><xsd:element name="businessInd">	<xsd:simpleType>	  <xsd:restriction base="xsd:string">		<xsd:enumeration value='B'/> <!-- 'Business'-->		<xsd:enumeration value='C'/> <!-- 'Consumer'-->		<xsd:enumeration value='D'/> <!-- 'Business and Consumer' -->	  </xsd:restriction>	</xsd:simpleType></xsd:element><xsd:element name="businessName" type="xsd:string"/><xsd:simpleType name="sourceType">	<xsd:restriction base="xsd:string">		<xsd:enumeration value='dtmf'/>			<!-- dtmf input -->		<xsd:enumeration value='speech'/>		<!-- spoken input-->		<xsd:enumeration value='tpd'/>			<!-- third-party data -->	</xsd:restriction></xsd:simpleType><xsd:element name="estimatedDelay" type="xsd:integer">	<xsd:annotation>		<xsd:documentation xml:lang="en">Estimated wait time till the completion of the session (sec). The way session results are delivered is application-dependant.</xsd:documentation>	</xsd:annotation></xsd:element><xsd:complexType name="namedDataResultType">	<xsd:sequence>		<xsd:element name="dataItem" minOccurs="1" maxOccurs="unbounded">			<xsd:complexType>				<xsd:simpleContent>					<xsd:extension base="xsd:string">						<xsd:attribute name="name" type="xsd:string" use="required"/>						<xsd:attribute name="match" type="tns:matchType" use="optional"/>						<xsd:attribute name="confidence" type="tns:confidenceType" use="optional"/>						<xsd:attribute name="source" type="tns:sourceType" use="optional"/>					</xsd:extension>				</xsd:simpleContent>			</xsd:complexType>		</xsd:element>	</xsd:sequence></xsd:complexType><xsd:element name="namedDataResult" type="tns:namedDataResultType"/><xsd:simpleType name="inServiceType">	<xsd:annotation>		<xsd:documentation xml:lang="en">InService Query Result</xsd:documentation>	</xsd:annotation>	<xsd:restriction base="xsd:integer">		<xsd:enumeration value='0'/> <!-- 'Not In Service For The Requested Time' -->		<xsd:enumeration value='1'/> <!-- 'In Service For The Requested Time'-->		<xsd:enumeration value='2'/> <!-- 'Unknown' -->	</xsd:restriction></xsd:simpleType><xsd:element name="inService" type="tns:inServiceType"/><!-- Archive only ==================================================== --><!-- ================================================================= --><xsd:complexType name="recordingsType">	<xsd:sequence>		<xsd:element name="recording" minOccurs="1" maxOccurs="unbounded">			<xsd:complexType>				<xsd:sequence>					<xsd:element name="fileName" type="xsd:string"/>					<xsd:element name="encoding" type="xsd:integer"/>   <!-- Should be enum -->					<xsd:element name="length" type="xsd:integer"/>					<xsd:element name="timestamp" type="xsd:dateTime"/>					<xsd:element name="binaryContent" type="xsd:base64Binary"/>				</xsd:sequence>			</xsd:complexType>		</xsd:element>	</xsd:sequence></xsd:complexType><xsd:element name="recordings" type="tns:recordingsType"/></xsd:schema>