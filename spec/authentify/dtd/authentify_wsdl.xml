sd:string" minOccurs="0" />		        <xsd:element name="relationshipExpiration" type="xsd:integer" minOccurs="0" />               <xsd:element name="appAuthExpiry" type="xsd:integer" minOccurs="0" />               <xsd:element name="appReqAuth" type="xsd:integer" minOccurs="0" />               <xsd:element name="appAuthUpdated" type="xsd:integer" minOccurs="0" />               <xsd:element name="waitForInformAck" type="xsd:boolean" minOccurs="0" />			</xsd:sequence>		</xsd:complexType>      <xsd:complexType name="biometricInfo">        <xsd:annotation>          <xsd:documentation xml:lang="en">                    Biometric Info.                </xsd:documentation>        </xsd:annotation>        <xsd:all>          <xsd:element minOccurs="0" name="securityLevel" type="tns:securityLevelType" />          <xsd:element name="VID" type="xsd:string" />        </xsd:all>      </xsd:complexType>		<xsd:element name="deviceContact" type="tns:deviceContactType"/>		<xsd:complexType name="deviceContactType">			<xsd:simpleContent>				<xsd:extension base="xsd:string">					<xsd:attribute name="registered" type="xsd:boolean" />				</xsd:extension>			</xsd:simpleContent>		</xsd:complexType>		<xsd:element name="relationshipId" type="tns:relationshipIdType"/>		<xsd:complexType name="relationshipIdType">			<xsd:simpleContent>				<xsd:extension base="xsd:string">                   <xsd:attribute name="clientType" type="xsd:string" />                   <xsd:attribute name="expires" type="xsd:string" />				</xsd:extension>			</xsd:simpleContent>		</xsd:complexType>      <xsd:complexType name="phoneLookupInfo">        <xsd:annotation>          <xsd:documentation xml:lang="en">                    Information resulting from looking up the phone number in databases.                </xsd:documentation>        </xsd:annotation>        <xsd:all>          <xsd:element minOccurs="0" name="zipExchangeDistance" type="xsd:decimal" />          <xsd:element minOccurs="0" name="populationDensity" type="xsd:decimal" />          <xsd:element minOccurs="0" name="exchangeType">            <xsd:annotation>              <xsd:documentation>					   		Exchange Type of the phone number looked up.					   	</xsd:documentation>            </xsd:annotation>            <xsd:simpleType>              <xsd:restriction base="xsd:string">                <xsd:length value="2" />                <xsd:pattern value="\d{2}" />              </xsd:restriction>            </xsd:simpleType>          </xsd:element>          <xsd:element minOccurs="0" name="businessInd">            <xsd:annotation>              <xsd:documentation>                            Indicator to what type of phone this is.                            B: Business                            C: Consumer                            D: Business And Consumer                        </xsd:documentation>            </xsd:annotation>            <xsd:simpleType>              <xsd:restriction base="xsd:string">                <xsd:enumeration value="B" />                <xsd:enumeration value="C" />                <xsd:enumeration value="D" />              </xsd:restriction>            </xsd:simpleType>          </xsd:element>          <xsd:element minOccurs="0" name="businessName" type="xsd:string" />          <xsd:element minOccurs="0" name="inService" type="tns:inServiceType" />        </xsd:all>      </xsd:complexType>      <xsd:element name="RequestData">        <xsd:complexType>          <xsd:all>            <xsd:element name="action" type="xsd:string" />            <xsd:element minOccurs="0" name="transaction" type="tns:transactionInformation" />            <xsd:element minOccurs="0" name="subject" type="tns:subjectInformation" />            <xsd:element minOccurs="0" name="namedData" type="tns:namedDataType" />            <xsd:element name="phoneNumber" type="xsd:string" />            <xsd:element minOccurs="0" name="confirmNumber" type="xsd:string" />            <xsd:element minOccurs="0" name="redirectURL" type="xsd:string" />            <xsd:element name="xfaData" type="tns:XfaDataType" minOccurs="0"/>          </xsd:all>        </xsd:complexType>      </xsd:element>      <xsd:element name="PollData">        <xsd:complexType>          <xsd:sequence>            <xsd:element name="transaction" type="tns:transactionInformation" />          </xsd:sequence>        </xsd:complexType>      </xsd:element>      <xsd:element name="ResponseData">        <xsd:complexType>          <xsd:all>            <xsd:element name="replyTo" type="tns:urlString" />            <xsd:element name="action" type="xsd:string" />            <xsd:element name="responseType" type="tns:responseTypeType" />            <xsd:element minOccurs="0" name="namedData" type="tns:namedDataType" />            <xsd:element name="transaction" type="tns:transactionInformation" />            <xsd:element minOccurs="0" name="phoneLookup" type="tns:phoneLookupInfo" />            <xsd:element minOccurs="0" name="subject" type="tns:subjectInformation" />            <xsd:element minOccurs="0" name="phoneNumber" type="xsd:string" />            <xsd:element minOccurs="0" name="confirmNumber" type="xsd:string" />            <xsd:element minOccurs="0" name="phoneCallDuration" type="xsd:nonNegativeInteger" />            <xsd:element name="xfaData" type="tns:XfaDataType" minOccurs="0"/>            <xsd:element name="status">              <xsd:complexType>                <xsd:sequence>                  <xsd:element name="statusCode" type="tns:hexString" />                  <xsd:element minOccurs="0" name="phoneValidityStatus" type="tns:hexString" />                  <xsd:element minOccurs="0" name="phoneLookupStatus" type="tns:hexString" />                  <xsd:element minOccurs="0" name="statusText" type="xsd:string" />                </xsd:sequence>              </xsd:complexType>            </xsd:element>          </xsd:all>        </xsd:complexType>      </xsd:element>      <xsd:element name="authentication" type="tns:authenticationHeader">        <xsd:annotation>          <xsd:documentation>                    The authentication header used in a request message                    for authentify                </xsd:documentation>        </xsd:annotation>      </xsd:element>    </xsd:schema>  </wsdl:types>  <wsdl:message name="RequestAuth">    <wsdl:part name="authentication_header" element="tns:authentication" />  </wsdl:message>  <wsdl:message name="RequestRequest">    <wsdl:part name="data" element="tns:RequestData" />  </wsdl:message>  <wsdl:message name="PollRequest">    <wsdl:part name="data" element="tns:PollData" />  </wsdl:message>  <wsdl:message name="RequestResponse1">    <wsdl:part name="data" element="tns:ResponseData" />  </wsdl:message>  <wsdl:portType name="AuthentifyService">    <wsdl:operation name="Request">      <wsdl:input message="tns:RequestRequest" />      <wsdl:output message="tns:RequestResponse1" />    </wsdl:operation>    <wsdl:operation name="Poll">      <wsdl:input message="tns:PollRequest" />      <wsdl:output message="tns:RequestResponse1" />    </wsdl:operation>  </wsdl:portType>  <wsdl:binding name="AuthentifyServiceBinding" type="tns:AuthentifyService">    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" />    <wsdl:operation name="Request">      <soap:operation soapAction="http://xml.authentify.net/AuthentifyService/Request" />      <wsdl:input>        <soap:body use="literal" />        <soap:header message="tns:RequestAuth" part="authentication_header" use="literal" />      </wsdl:input>      <wsdl:output>        <soap:body use="literal" />      </wsdl:output>    </wsdl:operation>    <wsdl:operation name="Poll">      <soap:operation soapAction="http://xml.authentify.net/AuthentifyService/Poll" />      <wsdl:input>        <soap:body use="literal" />      </wsdl:input>      <wsdl:output>        <soap:body use="literal" />      </wsdl:output>    </wsdl:operation>  </wsdl:binding>  <wsdl:service name="AuthentifyService">    <wsdl:port name