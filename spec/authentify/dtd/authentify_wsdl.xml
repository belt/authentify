<?xml version="1.0" encoding="utf-8"?><wsdl:definitions xmlns:tns="http://xml.authentify.net/AuthentifyService/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="AuthentifyService" targetNamespace="http://xml.authentify.net/AuthentifyService/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">  <wsdl:types>    <xsd:schema targetNamespace="http://xml.authentify.net/AuthentifyService/" elementFormDefault="qualified">      <xsd:simpleType name="urlString">        <xsd:restriction base="xsd:string">          <xsd:maxLength value="1024" />        </xsd:restriction>      </xsd:simpleType>      <xsd:simpleType name="hexString">        <xsd:restriction base="xsd:string">          <xsd:minLength value="1" />          <xsd:pattern value="[0-9a-fA-F]+" />        </xsd:restriction>      </xsd:simpleType>      <xsd:simpleType name="securityLevelType">        <xsd:annotation>          <xsd:documentation xml:lang="en">                    Voice biometrics verification security ("confidence") level                    1: Very High                    2: High                    3: Medium High                    4: Medium                    5: Medium Low                </xsd:documentation>        </xsd:annotation>        <xsd:restriction base="xsd:integer">          <xsd:enumeration value="1" />          <xsd:enumeration value="2" />          <xsd:enumeration value="3" />          <xsd:enumeration value="4" />          <xsd:enumeration value="5" />        </xsd:restriction>      </xsd:simpleType>      <xsd:simpleType name="matchConfidenceType">        <xsd:restriction base="xsd:nonNegativeInteger">          <xsd:maxInclusive value="100" />        </xsd:restriction>      </xsd:simpleType>      <xsd:simpleType name="sourceType">        <xsd:annotation>          <xsd:documentation>The source of a match                dtmf:    dtmf input                speech:  spoken input                tpd:     third-party data                </xsd:documentation>        </xsd:annotation>        <xsd:restriction base="xsd:string">          <xsd:enumeration value="dtmf" />          <xsd:enumeration value="speech" />          <xsd:enumeration value="tpd" />        </xsd:restriction>      </xsd:simpleType>      <xsd:simpleType name="responseTypeType">        <xsd:annotation>          <xsd:documentation>                    The type of response                </xsd:documentation>        </xsd:annotation>        <xsd:restriction base="xsd:string">          <xsd:enumeration value="report" />          <xsd:enumeration value="result" />        </xsd:restriction>      </xsd:simpleType>      <xsd:simpleType name="inServiceType">        <xsd:annotation>          <xsd:documentation xml:lang="en">                    InService Query Result                    1: Not In Service For The Requested Time                    2: In Service For The Requested Time                    3: Unknown                </xsd:documentation>        </xsd:annotation>        <xsd:restriction base="xsd:integer">          <xsd:enumeration value="0" />          <xsd:enumeration value="1" />          <xsd:enumeration value="2" />        </xsd:restriction>      </xsd:simpleType>      <xsd:complexType name="matchStringType">        <xsd:annotation>          <xsd:documentation xml:lang="en">                    An element of a form </xsd:documentation>        </xsd:annotation>        <xsd:simpleContent>          <xsd:extension base="xsd:string">            <xsd:attribute name="match" type="tns:matchType" use="optional" />          </xsd:extension>        </xsd:simpleContent>      </xsd:complexType>	<xsd:complexType name="matchCountryType">		<xsd:simpleContent>			<xsd:restriction base="tns:matchStringType">				<xsd:pattern value="[A-Z]{2}" />			</xsd:restriction>		</xsd:simpleContent>	</xsd:complexType>      <xsd:group name="authenticationHeaderInner">        <xsd:sequence>          <xsd:element name="tsoid">            <xsd:simpleType>              <xsd:restriction base="xsd:string">                <xsd:maxLength value="16" />              </xsd:restriction>            </xsd:simpleType>          </xsd:element>          <xsd:element name="application">            <xsd:simpleType>              <xsd:restriction base="xsd:string">                <xsd:maxLength value="32" />              </xsd:restriction>            </xsd:simpleType>          </xsd:element>          <xsd:element minOccurs="0" name="account">            <xsd:simpleType>              <xsd:restriction base="xsd:string">                <xsd:maxLength value="32" />              </xsd:restriction>            </xsd:simpleType>          </xsd:element>        </xsd:sequence>      </xsd:group>      <xsd:complexType name="authenticationHeader">        <xsd:sequence>          <xsd:group ref="tns:authenticationHeaderInner" />          <xsd:choice>            <xsd:element name="password" type="xsd:string" />            <xsd:element name="licenseKey">              <xsd:simpleType>                <xsd:restriction base="xsd:string">                  <xsd:maxLength value="170" />                </xsd:restriction>              </xsd:simpleType>            </xsd:element>          </xsd:choice>        </xsd:sequence>      </xsd:complexType>      <xsd:complexType name="transactionInformation">        <xsd:annotation>          <xsd:documentation>					Collection of Session Information					replyTo:      Where the session is running - OUT					teid:         Transaction ID - OUT					sgid:         Session Group ID - IN/OUT					timestamp:    Time of Session - OUT					billingGroup: Customer Supplied Billing Group ID - COPIED (IN to OUT)					sessionTag:	  Customer Supplied Session ID - COPIED (IN to OUT)					states:       States this transaction went through - OUT				</xsd:documentation>        </xsd:annotation>        <xsd:all>          <xsd:element minOccurs="0" name="delay">            <xsd:simpleType>              <xsd:restriction base="xsd:integer">                <xsd:maxInclusive value="3